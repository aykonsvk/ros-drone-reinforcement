<launch>
    <!-- This version uses the openai_ros environments -->
    <rosparam command="load" file="$(find reinforcement_drone )/config/left_right_qlearn_params_v0.yaml" />
    
    <include file="$(find reinforcement_drone)/launch/main.launch"/>


    <!-- Launch the training system -->
    <node pkg="reinforcement_drone" name="left_right_qlearn" type="left_right_qlearn_v0.py" output="screen"/>
</launch>